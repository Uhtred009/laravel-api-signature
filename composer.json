{
    "name": "mitoop/laravel-api-signature",
    "description": "laravel api signature",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "徐夏冰",
            "email": "037333@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "5.5.*|5.6.*|5.7.*",
        "moontoast/math": "^1.1"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Mitoop\\ApiSignature\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mitoop\\ApiSignature\\ClientServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
